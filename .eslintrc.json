{
	"env": {"browser": true},
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"project": "./tsconfig.json",
		"tsconfigRootDir": "."
	},
	"plugins": ["@typescript-eslint", "prettier"],
	"extends": [
		"xo",
		"xo-typescript",
		"plugin:prettier/recommended",
		"prettier/@typescript-eslint"
	],
	"rules": {
		"prettier/prettier": "error"
	},
	"globals": {
		"window": true
	}
}
